{{#ifwith resume.skills}}
	<section>
		<header>
			<h2>Skills</h2>
		</header>
		{{#each this}}
			<section>
				<header>
					{{#if name}}
						<h3>{{name}}</h3>
					{{/if}}
				</header>
				{{#if level}}
					{{level}}
				{{/if}}
				{{#ifwith keywords}}
					<ul>
						{{#each this}}
							<li>{{.}}</li>
						{{/each}}
					</ul>
				{{/ifwith}}
			</section>
		{{/each}}
	</section>
{{/ifwith}}