{{#ifwith resume.languages}}
	<section>
		<header>
			<h2>Languages</h2>
		</header>
		<ul id="languages-list">
			{{#each this}}
				<li>
					<header>
						<h3>{{language}}</h3>
					</header>
					{{#if fluency}}
						<strong>Fluency:</strong> {{fluency}}
					{{/if}}
				</li>
			{{/each}}
		</ul>
	</section>
{{/ifwith}}