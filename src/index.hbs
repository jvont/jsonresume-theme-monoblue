<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <style>{{css}}</style>
</head>
<body>
  <header>
    {{#with about}}
      <h1>{{name}}</h1>
      <h2>{{title}}</h2>
    {{/with}}
  </header>
  <main>
    {{#each main}}
    <h1>{{@key}}</h1>
      {{#each this}}
        <div class="section">
          <h2>{{title}}</h2>
          <h3>{{subtitle}}</h3>
          <h4>{{start}} &mdash; {{end}}</h4>
          {{description}}
          <ul>
          {{#each notes}}
            <li>{{this}}</li>
          {{/each}}
          </ul>
        </div>
      {{/each}}
    {{/each}}
  </main>
  <aside>
    {{#each aside}}
    {{/each}}
  </aside>
  <footer>
    <ul>
      {{#each references}}
        <li>{{title}}, {{description}}</li>
      {{/each}}
    </ul>
  </footer>
</body>
</html>
